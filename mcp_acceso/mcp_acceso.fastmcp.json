{
  "$schema": "https://gofastmcp.com/public/schemas/fastmcp.json/v1.json",
  "source": {
    "path": "server.py",
    "entrypoint": "mcp"
  },
  "environment": {
    "python": "3.11",
    "dependencies": [
      "fastmcp>=2.0.0",
      "asyncpg>=0.29.0",
      "pytz>=2024.1"
    ]
  },
  "deployment": {
    "transport": "streamable-http",
    "host": "0.0.0.0",
    "port": 80,
    "log_level": "INFO",
    "env": {
      "TIMEZONE": "America/Bogota"
    }
  }
}
