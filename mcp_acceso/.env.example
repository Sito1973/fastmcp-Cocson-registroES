# Copiar este archivo a .env y configurar los valores

# URL de conexión a PostgreSQL (formato asyncpg)
DATABASE_URL_FALLBACK=postgresql+asyncpg://usuario:password@host:5432/database

# Zona horaria para cálculos de fechas
TIMEZONE=America/Bogota

# Puerto del servidor (para despliegue HTTP/SSE)
PORT=80
